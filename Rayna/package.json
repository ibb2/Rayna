    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.3.9",
    "lucide-react": "^0.554.0",
    "next-themes": "^0.4.6",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
